{
  "name": "dolthub-cypress",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "cy-open": "cypress open",
    "cy-run": "cypress run",
    "cy-chrome": "cypress run --browser chrome --headless",
    "cy-open-dev": "CYPRESS_BASE_URL=https://dolthub.awsdev.ld-corp.com cypress open",
    "cy-run-dev": "CYPRESS_BASE_URL=https://dolthub.awsdev.ld-corp.com cypress run",
    "cylo-open-dolthub": "CYPRESS_LOCAL_DOLTHUB=true && CYPRESS_NUM_TESTS_KEPT_IN_MEMORY=0 && CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "cylo-open-blog": "CYPRESS_LOCAL_BLOG=true && CYPRESS_BASE_URL=http://localhost:8000 cypress open",
    "cylo-run-dolthub": "CYPRESS_LOCAL_DOLTHUB=true && CYPRESS_BASE_URL=http://localhost:3000 cypress run",
    "cylo-run-blog": "CYPRESS_LOCAL_BLOG=true && CYPRESS_BASE_URL=http://localhost:8000 cypress run",
    "cy-open-doltlab": "CYPRESS_BASE_URL=http://0.0.0.0 cypress open integrationFolder=cypress/integration/doltlab/productionTests",
    "cy-run-doltlab": "CYPRESS_BASE_URL=http://0.0.0.0 cypress run integrationFolder=cypress/integration/doltlab/productionTests",
    "cy-chrome-doltlab": "cypress run integrationFolder=cypress/integration/doltlab/productionTests --browser chrome --headless",
    "ci": "npm-run-all prettier compile lint",
    "compile": "tsc -b",
    "lint": "eslint --cache --ext .ts,.js .",
    "prettier": "prettier --check 'cypress/**/*.{ts,js}'",
    "prettier:fix": "prettier --write 'cypress/**/*.{ts,js}'",
    "postinstall": "bash -c 'if [ -z \"$HUSKY_SKIP_INSTALL\" ]; then husky install .husky; fi'"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cypress/github-action": "^3.0.3",
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.33",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "yarnhook": "^0.5.1"
  },
  "dependencies": {
    "cypress": "^9.6.0",
    "cypress-file-upload": "^5.0.8",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "minimist": "^1.2.6"
  }
}
